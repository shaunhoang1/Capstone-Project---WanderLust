                                  "meta": {
                                    "heading": "Image (will scale to column width)",
                                    "subHeading": "Recommended: JPEG @ 900px wide"
                                  },
                                  "isGenerated": false,
                                  "isUserDefined": false
                                },
                                "altText": "",
